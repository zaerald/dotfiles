{
  "languageserver": {
    "sql": {
      "command": "sql-language-server",
      "args": [ "up", "--method", "stdio" ],
      "filetypes": [ "sql", "mysql" ]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}

